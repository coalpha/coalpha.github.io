{
   "version": "0.2.0",
   "configurations": [
      {
         "type": "java",
         "name": "Launch coalpha/BoxingConversion",
         "request": "launch",
         "mainClass": "coalpha/BoxingConversion",
         "projectName": "coalpha-java"
      },
      {
         "name": "snake!",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "integratedTerminal"
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "lldb++",
         "program": "${workspaceFolder}/cxx/${fileBasenameNoExtension}.debug.exe",
         "cwd": "${workspaceFolder}"
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "lldb",
         "program": "${workspaceFolder}/c/${fileBasenameNoExtension}.debug.exe",
         "cwd": "${workspaceFolder}",
         "terminal": "external",
         "initCommands": [
            "settings set target.x86-disassembly-flavor intel"
         ]
      }
   ]
