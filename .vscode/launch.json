{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "snake!",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "integratedTerminal"
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "lldb++",
         "program": "${workspaceFolder}/cxx/${fileBasenameNoExtension}.debug.exe",
         "cwd": "${workspaceFolder}",
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "lldb",
         "program": "${workspaceFolder}/c/${fileBasenameNoExtension}.debug.exe",
         "cwd": "${workspaceFolder}",
         "terminal": "external",
         "initCommands": ["settings set target.x86-disassembly-flavor intel"]
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "lldb for assembly",
         "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
         "cwd": "${fileDirname}",
         "initCommands": ["settings set target.x86-disassembly-flavor intel"]
      },
   ]
}
